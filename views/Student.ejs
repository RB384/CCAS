			<table style="width:70%; margin-left:10%" >
			<tr>
			  <td style="width:35%">SID:</td>
			  <td style="width:40%"> <input type="text" style="background:#dddddd;" class="input100"   name="SID" value="<%= data.SID %>" disabled></td>
			  <td></td>
			</tr>

			<tr>
			  <td>Name:</td>
			  <td> <input type="text"  class="input100"   name="Name" style="background:#dddddd;"  value="<%= data.name %>" disabled></td>
			  <td></td>
			</tr>

			<tr>
			  <td>Father's Name:</td>
			   <td> <input type="text" style="background:#dddddd;" class="input100"   name="Father_Name"
					 value="<%= data.father_name %>" disabled></td>
			   <td></td>
			</tr>

			<tr>
			  <td>Address:</td>
			  <td> <textarea name ="address" ng-model="address" rows="3" class="input100" ></textarea></td>
			  <td></td>
			</tr>

			<tr>
			  <td>Email:</td>
			  <td><input type="email" name="Email"  class="input100"  required ng-model="Email"></td>
			  <td> <span ng-show="Request.Email.$dirty  && Request.Email.$invalid"><h4><b><p style="color:red">
					Enter valid Email</p></b></h4></span></td>
			</tr>

			<tr>
			  <td>Mobile Number</td>
			  <td> <input type="text"  class="input100" name="Mobile"
					required ng-model="Mobile"></td>
			  <td><span ng-show="Request.Mobile.$dirty && Request.Mobile.$invalid"><h4><b><p style="color:red">Enter valid Mobile number</p></b></h4></span>  </td>
			</tr>

			<tr>
			  <td>UG_PG:</td>
			   <td> <input type="text"  style="background:#dddddd;" class="input100"  name="UG_PG"  value="<%= data.UG_PG %>" disabled></td>
			   <td></td>
			</tr>

			<tr>
			  <td>Branch:</td>
			  <td> <input type="text" style="background:#dddddd;"  class="input100" name="Branch"  disabled value="<%= data.Branch %>"></td>
			  <td></td>
			</tr>

			<tr>
			  <td>Year :</td>
			  <td>
			  <select  name="Year"  ng-model="Year" class="input100" required>
			  <option value="Select Year">Select Year</option>
			  <option value="First">First</option>
			  <option value="Second">Second</option>
			  <option value="Third">Third</option>
			  <option value="Fourth">Fourth</option>
			  </select>
			</td>
			<td></td>
			</tr>

			<tr>
			  <td>CGPA:</td>
			  <td> <input type="text"  class="input100"  placeholder="" name="CGPA"  ng-model="CGPA" required></td>
			  <td></td>
			</tr>

			<tr>
			  <td>Backlog :</td>
			  <td >
			  <select  name="Backlog"  ng-model="Backlog" class="input100" required >
			   <option value="Yes">Yes</option>
			   <option value="No">No</option>
			  </select>
			</td>
			<td></td>
			</tr>

			<tr rowspan=2>
			  <td>Disciplinary Action<br> taken against You:</td>
