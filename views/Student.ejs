}
#nam {
		font-size: 27px; text-align: center;
}
.split {
  height: 100%; position: fixed;
  z-index: 1; top: 0; overflow-x: hidden;padding-top: 20px;
}

.left {left: 0;}
.right {right: 0;}
.anch a:hover, a:active{
    text-decoration: none;
    display: inline-block;
    color: black;

}
.input100 {
  display: block;
  width: 100%;
  height: 50px;
